<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:id="@id/wifi_history_graph_label_group"
        android:layout_width="fill_parent"
        android:layout_height="0.0dip"
        android:orientation="horizontal"
        android:clipChildren="false"
        android:clipToPadding="false"
        android:layout_weight="1.0">

        <com.samsung.android.settings.wifi.develop.history.view.HistoryGraph
            android:id="@id/wifi_history_graph"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginTop="@dimen/usage_graph_margin_top_bottom"
            android:layout_marginBottom="@dimen/usage_graph_margin_top_bottom"
            android:layout_weight="1.0" />

        <LinearLayout
            android:id="@id/wifi_history_label_group"
            android:layout_width="35.0dip"
            android:layout_height="fill_parent"
            android:gravity="end"
            android:orientation="vertical">

            <TextView
                android:id="@id/wifi_history_label_top"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:textAppearanceSmall"
                android:contentDescription="@null"
                android:hyphenationFrequency="normal" />

            <Space
                android:id="@id/history_space1"
                android:layout_width="wrap_content"
                android:layout_height="0.0dip"
                android:layout_weight="1.0" />

            <TextView
                android:id="@id/wifi_history_label_middle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:textAppearanceSmall"
                android:contentDescription="@null"
                android:hyphenationFrequency="normal" />

            <Space
                android:id="@id/wifi_history_space2"
                android:layout_width="wrap_content"
                android:layout_height="0.0dip"
                android:layout_weight="1.0" />

            <TextView
                android:id="@id/wifi_history_label_bottom"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:textAppearanceSmall"
                android:contentDescription="@null"
                android:hyphenationFrequency="normal" />

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@id/wifi_history_bottom_label_group"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0.0dip"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_weight="1.0">

            <TextView
                android:id="@id/wifi_history_label_start"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:textAppearanceSmall"
                android:contentDescription="@null"
                android:hyphenationFrequency="normal" />

            <Space
                android:id="@id/history_spacer"
                android:layout_width="40.0dip"
                android:layout_height="wrap_content"
                android:layout_weight="1.0" />

            <TextView
                android:id="@id/wifi_history_label_end"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:textAppearanceSmall"
                android:contentDescription="@null"
                android:hyphenationFrequency="normal" />

            <Space
                android:id="@id/wifi_history_bottom_label_space"
                android:layout_width="30.0dip"
                android:layout_height="wrap_content" />

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@id/wifi_history_legend1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center" />

        <TextView
            android:id="@id/wifi_history_legend2"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center" />

    </LinearLayout>

</LinearLayout>
