<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@id/tap_layout"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@id/tap"
        android:layout_width="@dimen/touch_circle_cue_size"
        android:layout_height="@dimen/touch_circle_cue_size"
        android:layout_gravity="center"
        android:src="@drawable/circle_hold" />

    <ImageView
        android:id="@id/tap_ignore"
        android:layout_width="@dimen/touch_circle_cue_size"
        android:layout_height="@dimen/touch_circle_cue_size"
        android:layout_gravity="center"
        android:src="@drawable/circle_fail" />

</FrameLayout>
