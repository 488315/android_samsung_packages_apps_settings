<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:settings="http://schemas.android.com/apk/res-auto">
    <com.android.settingslib.widget.LayoutPreference android:layout="@layout/sec_wifi_edittext_with_errortext" android:selectable="false" android:key="ssid" />
    <androidx.preference.DropDownPreference android:title="@string/wifi_security" android:key="security_settings" />
    <com.android.settingslib.widget.LayoutPreference android:layout="@layout/sec_wifi_edittext_with_errortext" android:selectable="false" android:key="eap_identity" />
    <com.android.settingslib.widget.LayoutPreference android:layout="@layout/sec_wifi_password_edittext" android:selectable="false" android:key="wifi_password" />
    <androidx.preference.DropDownPreference android:title="@string/wifi_eap_ca_cert" android:key="eap_ca_cert" />
    <com.samsung.android.settings.wifi.SecWifiUnclickablePreference android:selectable="false" android:key="eap_ca_cert_error_text" style="@style/sec_wifi_description_sub_text" />
    <com.android.settingslib.widget.LayoutPreference android:layout="@layout/sec_wifi_edittext" android:selectable="false" android:key="eap_domain" />
    <com.samsung.android.settings.wifi.SecWifiUnclickablePreference android:selectable="false" android:key="wifi_eap_ca_error_text" style="@style/sec_wifi_description_sub_text" />
    <androidx.preference.DropDownPreference android:title="@string/wifi_eap_user_cert" android:key="eap_user_cert" />
    <com.samsung.android.settings.widget.SecButtonPreference android:layout_alignParentBottom="true" android:key="save_button" />
    <androidx.preference.SwitchPreferenceCompat android:title="@string/wifi_auto_reconnect" android:key="auto_connect" />
    <androidx.preference.DropDownPreference android:title="@string/wifi_select_sim" android:key="eap_sim_select" />
    <com.samsung.android.settings.widget.SecInsetCategoryPreference />
    <androidx.preference.SwitchPreferenceCompat android:title="@string/wifi_hidden_network" android:key="hidden" />
    <com.android.settingslib.widget.LayoutPreference android:layout="@layout/sec_wifi_preference_error_text" android:selectable="false" android:key="hidden_warning_text" />
    <com.samsung.android.settings.widget.SecInsetCategoryPreference android:key="category_view_more" />
    <com.samsung.android.settings.wifi.details.WifiExpandablePreference android:title="wifi_menu_advanced" android:key="expander" />
    <PreferenceCategory android:key="advanced">
        <androidx.preference.DropDownPreference android:entries="@array/sec_wifi_eap_method" android:title="@string/wifi_eap_method" android:key="eap_method" android:entryValues="@array/sec_wifi_eap_method_values" />
        <androidx.preference.DropDownPreference android:title="@string/wifi_eap_min_tls_ver" android:key="eap_min_tls_ver" />
        <androidx.preference.DropDownPreference android:entries="@array/sec_wifi_eap_ocsp" android:title="@string/wifi_eap_ocsp" android:key="eap_ocsp" android:entryValues="@array/sec_wifi_eap_ocsp_values" />
        <androidx.preference.DropDownPreference android:entries="@array/wifi_ttls_phase2_entries" android:title="@string/please_select_phase2" android:key="eap_phase2" android:entryValues="@array/wifi_phase2_values" settings:isPreferenceVisible="false" />
        <com.android.settingslib.widget.LayoutPreference android:layout="@layout/sec_wifi_edittext_with_errortext" android:selectable="false" android:key="eap_anonymous" />
        <com.samsung.android.settings.widget.SecInsetCategoryPreference android:key="eap_inset" />
        <PreferenceCategory android:layout="@layout/preference_category_no_label" android:key="network_config">
            <androidx.preference.DropDownPreference android:entries="@array/wifi_ip_settings" android:title="@string/wifi_ip_settings" android:key="ip_settings" android:entryValues="@array/wifi_ip_values" />
            <com.android.settingslib.widget.LayoutPreference android:layout="@layout/sec_wifi_edittext" android:selectable="false" android:key="wifi_ip_address" settings:isPreferenceVisible="false" />
            <com.android.settingslib.widget.LayoutPreference android:layout="@layout/sec_wifi_edittext" android:selectable="false" android:key="gateway" settings:isPreferenceVisible="false" />
            <com.android.settingslib.widget.LayoutPreference android:layout="@layout/sec_wifi_edittext" android:selectable="false" android:key="wifi_network_prefix_length" settings:isPreferenceVisible="false" />
            <com.android.settingslib.widget.LayoutPreference android:layout="@layout/sec_wifi_edittext" android:selectable="false" android:key="wifi_dns1" settings:isPreferenceVisible="false" />
            <com.android.settingslib.widget.LayoutPreference android:layout="@layout/sec_wifi_edittext" android:selectable="false" android:key="wifi_dns2" settings:isPreferenceVisible="false" />
            <com.samsung.android.settings.widget.SecInsetCategoryPreference android:key="ip_inset" />
            <androidx.preference.DropDownPreference android:entries="@array/wifi_proxy_settings" android:title="@string/proxy_settings_title" android:key="proxy_settings" android:entryValues="@array/wifi_proxy_values" />
            <com.samsung.android.settings.wifi.SecWifiUnclickablePreference android:title="@string/proxy_warning_limited_support" android:selectable="false" android:key="proxy_warning_limited_support" settings:isPreferenceVisible="false" style="@style/sec_wifi_description_sub_proxy" />
            <com.android.settingslib.widget.LayoutPreference android:layout="@layout/sec_wifi_edittext" android:selectable="false" android:key="proxy_pac" settings:isPreferenceVisible="false" />
            <com.android.settingslib.widget.LayoutPreference android:layout="@layout/sec_wifi_edittext" android:selectable="false" android:key="proxy_hostname" settings:isPreferenceVisible="false" />
            <com.android.settingslib.widget.LayoutPreference android:layout="@layout/sec_wifi_edittext" android:selectable="false" android:key="proxy_port" settings:isPreferenceVisible="false" />
            <com.android.settingslib.widget.LayoutPreference android:layout="@layout/sec_wifi_edittext" android:selectable="false" android:key="proxy_exclusionlist" settings:isPreferenceVisible="false" />
            <com.samsung.android.settings.widget.SecInsetCategoryPreference android:key="proxy_inset" />
        </PreferenceCategory>
        <androidx.preference.DropDownPreference android:entries="@array/wifi_metered_entries" android:title="@string/data_usage_metered_yes" android:key="metered" android:entryValues="@array/wifi_metered_values" />
        <androidx.preference.DropDownPreference android:entries="@array/wifi_privacy_entries" android:title="@string/wifi_privacy_settings" android:key="privacy" android:entryValues="@array/wifi_privacy_values" />
        <com.android.settingslib.widget.LayoutPreference android:layout="@layout/sec_wifi_preference_error_text" android:selectable="false" android:key="privacy_warning_text" />
        <androidx.preference.SwitchPreferenceCompat android:title="@string/wifi_connect_send_device_name_to_network" android:key="dhcp_name_sharing" />
    </PreferenceCategory>
    <com.samsung.android.settings.widget.SecInsetCategoryPreference />
    <com.samsung.android.settings.wifi.SecWifiProgressButtonPreference android:layout_alignParentBottom="true" android:selectable="false" android:key="button" />
    <com.samsung.android.settings.wifi.SecWifiProfileShareButtonPreference android:layout_alignParentBottom="true" android:selectable="false" android:key="shared_password" />
</PreferenceScreen>
