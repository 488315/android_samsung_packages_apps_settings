<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="@color/as_progress_indeterminate_bg" />
            <corners android:radius="@dimen/as_file_operation_progress_corner" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <solid android:color="@color/as_color_primary" />
                <corners android:radius="@dimen/as_file_operation_progress_corner" />
            </shape>
        </clip>
    </item>
</layer-list>
